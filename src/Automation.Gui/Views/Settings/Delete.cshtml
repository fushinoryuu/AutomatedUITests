@model Automation.Database.Model.setting

@{
    ViewBag.Title = "Delete";
}

<br />
<br />
<br />

<h2>Are you sure you want to delete this?</h2>

<div>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Target Browser
        </dt>
        <dd>
            @Html.DisplayFor(model => model.targetBrowser)
        </dd>
        <dt>
            Operating System
        </dt>
        <dd>
            @Html.DisplayFor(model => model.operatingSystem)
        </dd>
        <dt>
            Selenium Hub Uri
        </dt>
        <dd>
            @Html.DisplayFor(model => model.seleniumHubUri)
        </dd>

        <dt>
            Screenshot Folder
        </dt>
        <dd>
            @Html.DisplayFor(model => model.screenshotFolder)
        </dd>
    </dl>

    <hr />

    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()

        <div>
            <a class="btn btn-primary" href="@Url.Action("Index")">
                <i class="glyphicon glyphicon-arrow-left"></i>
                <span>Back to List</span>
            </a>
            <button class="btn btn-danger" type="submit" value="Delete">
                <span class="glyphicon glyphicon-trash"></span>
                <span>Delete</span>
            </button>
        </div>
    }
</div>